{
  "name": "resume_v4",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:nr": "cross-env NODE_OPTIONS=\"--require ./newrelic.js\" next dev --turbopack",
    "dev:win": "set \"NODE_OPTIONS=--require ./newrelic.js\" && next dev --turbopack",
    "dev:unix": "NODE_OPTIONS=\"--require ./newrelic.js\" next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "start:nr": "cross-env NODE_OPTIONS=\"--require ./newrelic.js\" next start",
    "start:win": "set \"NODE_OPTIONS=--require ./newrelic.js\" && next start",
    "start:unix": "NODE_OPTIONS=\"--require ./newrelic.js\" next start",
    "start:prod": "cross-env NODE_ENV=production NODE_OPTIONS=\"--require ./newrelic.js\" next start",
    "start:prod:win": "set NODE_ENV=production && set \"NODE_OPTIONS=--require ./newrelic.js\" && next start",
    "start:prod:unix": "NODE_ENV=production NODE_OPTIONS=\"--require ./newrelic.js\" next start",
    "lint": "eslint",
    "thumbnails": "node scripts/generate-thumbnails.mjs public/screenshots public/thumbs"
  },
  "dependencies": {
    "@marsidev/react-turnstile": "^1.3.1",
    "@sparticuz/chromium": "^138.0.2",
    "@upstash/redis": "^1.35.3",
    "groq-sdk": "^0.32.0",
    "newrelic": "^13.3.2",
    "next": "15.5.2",
    "nodemailer": "^7.0.6",
    "puppeteer": "^24.20.0",
    "puppeteer-core": "^24.20.0",
    "react": "19.1.0",
    "react-confetti": "^6.4.0",
    "react-dom": "19.1.0",
    "sharp": "^0.33.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/nodemailer": "^7.0.1",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cross-env": "^10.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
